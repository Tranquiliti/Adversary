						"planetaryshield",
						"waystation",
						"cryosanctum",
						"cryorevival",
					],

					# A map of industries, with values being the items to install in a given industry
					# Each entry in "specials" uses this format: 
					# "industryToModify":["special_items","ai_core_id"],
					# Special items ids are found in starsector-core/data/campaign/special_items.csv
					# Defaults to no installed items if this entry is omitted
					"specials":{
						#"heavyindustry":["corrupted_nanoforge",""],
						"orbitalworks":["pristine_nanoforge","gamma_core"],
						"fuelprod":["synchrotron","beta_core"],
						"population":["orbital_fusion_lamp","alpha_core"],
