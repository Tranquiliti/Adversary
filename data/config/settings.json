{
	# Allows the Adversary faction to change fleet doctrines every cycle
	# Requires a new game for changes to take effect
	"enableAdversaryDoctrineChange":true, # Default: true

	# Generate the Optimal star system upon starting a new game
	"enableOptimalStarSystem":true, # Default: true

	# Generates any custom star systems upon starting a new game
	"enableCustomStarSystems":true, # Default: true

	# If "enableDefaultStableEntities" is true in a star system:
	# First planet orbiting the center has a jump-point, inactive gate, and comm relay in its stable points
	# Second planet orbiting the center has a nav buoy and sensor array in its stable points
	"optimalStarSystem":{
		"enableDefaultStableEntities":true,
		"stars":[ # Stars in the center of the star system
			# Having more than one will make the stars orbit the center
			{	"type":"star_blue_giant",
				"radius":1100, # Star size
				"coronaSize":500,
			},
			{	"type":"star_yellow",
				"radius":800,
				"coronaSize":400,
			},
			{	"type":"star_orange",
				"radius":650,
				"coronaSize":400,
			},
		],
		"starsOrbitRadius":1600, # Orbit radius of center stars, if there is more than one
		"addCoronalHypershunt":false, # Adds an unactivated coronal hypershunt in this system
		"addDomainCryosleeper":false, # Adds an undisturbed Domain-era cryosleeper somewhere in this system
		"enableRandomLocation":false, # Does this system generate in a random constellation?
		"fringeJumpPointOrbitRadius":9200, # Distance away from center; determines system size in hyperspace
		"planetList":[
			{	"type":"gas_giant", # Planet type
				"focus":0, # 0 = orbits center, 1 = orbits 1st planet, 2 = orbits 2nd planet,...
				"radius":300, # Planet size
				"orbitRadius":4800, # How far this planet orbits the focus
				"conditions":[ # Can leave it empty (i.e. as []), to get proc-gen conditions
					"volatiles_plentiful",
					"high_gravity",
					"very_hot",
				],
				"marketSize":6, # Set to 0 to make this planet uninhabited
				# Planet options below only take effect if marketSize is at least 1
				"factionId":"adversary", # Who owns this planet?
				"aiCoreAdmin":true, # Is this market governed by an AI admin?
				"industries":[
					"population",
					"megaport",
					"waystation",
					"heavybatteries",
					"highcommand",
					"starfortress",
					"mining",
					"refining",
					"lightindustry",
				],
				"specials":{ # Colony items
